<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <bitmap android:gravity="center" android:src="@drawable/filtershow_curves_rgb_pressed" />
    </item>
    <item android:state_checked="true">
        <bitmap android:gravity="center" android:src="@drawable/filtershow_curves_rgb_pressed" />
    </item>
    <item>
        <bitmap android:gravity="center" android:src="@drawable/filtershow_curves_rgb_normal" />
    </item>
</selector>