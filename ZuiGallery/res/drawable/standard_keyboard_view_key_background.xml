<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/standard_keyboard_view_key_press_bg_color" />
            <corners android:radius="@dimen/standard_keyboard_view_key_rectangle_radius_corner" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/standard_keyboard_view_normal_key_bg_color" />
            <corners android:radius="@dimen/standard_keyboard_view_key_rectangle_radius_corner" />
            <stroke android:width="1.0px" android:color="@color/standard_keyboard_view_normal_key_stroke_color" />
        </shape>
    </item>
</selector>