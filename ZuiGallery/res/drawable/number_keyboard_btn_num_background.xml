<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/btn_number_keyboard_num_pressed_color" />
            <stroke android:width="1.0px" android:color="@color/btn_number_keyboard_num_pressed_stroke_color" />
            <corners android:radius="7.0dip" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/btn_number_keyboard_num_normal_color" />
            <stroke android:width="1.0px" android:color="@color/btn_number_keyboard_num_normal_stroke_color" />
            <corners android:radius="7.0dip" />
        </shape>
    </item>
</selector>