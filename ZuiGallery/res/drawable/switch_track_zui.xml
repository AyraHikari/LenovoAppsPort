<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_checked="true" android:state_pressed="true">
        <bitmap android:gravity="center" android:src="@drawable/switch_bg_focused_pressed_zui" />
    </item>
    <item android:state_enabled="true" android:state_checked="false" android:state_pressed="true">
        <bitmap android:gravity="center" android:src="@drawable/switch_bg_pressed_zui" />
    </item>
    <item android:state_enabled="true" android:state_checked="true">
        <bitmap android:gravity="center" android:src="@drawable/switch_bg_focused_zui" />
    </item>
    <item android:state_enabled="true" android:state_checked="false">
        <bitmap android:gravity="center" android:src="@drawable/switch_bg_zui" />
    </item>
    <item android:state_enabled="false" android:state_checked="true">
        <bitmap android:gravity="center" android:src="@drawable/switch_bg_focused_disabled_zui" />
    </item>
    <item android:state_enabled="false" android:state_checked="false">
        <bitmap android:gravity="center" android:src="@drawable/switch_bg_disabled_zui" />
    </item>
</selector>