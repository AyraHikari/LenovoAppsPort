<?xml version="1.0" encoding="utf-8"?>
<animated-selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_checked="false" android:id="@id/off">
        <nine-patch android:gravity="center" android:src="@drawable/switch_thumb_disabled_off_zui" />
    </item>
    <item android:state_enabled="false" android:state_checked="true" android:id="@id/on">
        <nine-patch android:gravity="center" android:src="@drawable/switch_thumb_disabled_on_zui" />
    </item>
    <item android:state_checked="true" android:id="@id/on">
        <nine-patch android:gravity="center" android:src="@drawable/btn_switch_to_on_mtrl_zui_00010" />
    </item>
    <item android:id="@id/off">
        <nine-patch android:gravity="center" android:src="@drawable/btn_switch_to_on_mtrl_zui_00000" />
    </item>
    <transition android:toId="@android:id/month_view" android:fromId="@android:id/mode_out" android:reversible="true">
        <animation-list>
            <item android:duration="15">
                <bitmap android:src="@drawable/btn_switch_to_on_mtrl_zui_00000" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/btn_switch_to_on_mtrl_zui_00001" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/btn_switch_to_on_mtrl_zui_00002" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/btn_switch_to_on_mtrl_zui_00003" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/btn_switch_to_on_mtrl_zui_00004" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/btn_switch_to_on_mtrl_zui_00005" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/btn_switch_to_on_mtrl_zui_00006" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/btn_switch_to_on_mtrl_zui_00007" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/btn_switch_to_on_mtrl_zui_00008" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/btn_switch_to_on_mtrl_zui_00009" />
            </item>
            <item android:duration="15">
                <bitmap android:src="@drawable/btn_switch_to_on_mtrl_zui_00010" />
            </item>
        </animation-list>
    </transition>
</animated-selector>