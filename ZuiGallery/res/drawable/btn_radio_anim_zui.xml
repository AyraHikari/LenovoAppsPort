<?xml version="1.0" encoding="utf-8"?>
<animated-selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_checked="true">
        <bitmap android:src="@drawable/btn_radio_to_on_mtrl_zui_009" android:alpha="?android:disabledAlpha" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/btn_radio_disabled_zui" android:alpha="?android:disabledAlpha" />
    </item>
    <item android:state_checked="true" android:id="@android:id/month_view">
        <bitmap android:src="@drawable/btn_radio_to_on_mtrl_zui_009" />
    </item>
    <item android:id="@android:id/mode_out">
        <bitmap android:src="@drawable/btn_radio_to_on_mtrl_zui_000" />
    </item>
    <transition android:toId="@android:id/month_view" android:fromId="@android:id/mode_out" android:reversible="true">
        <animation-list>
            <item android:duration="26">
                <bitmap android:src="@drawable/btn_radio_to_on_mtrl_zui_000" />
            </item>
            <item android:duration="26">
                <bitmap android:src="@drawable/btn_radio_to_on_mtrl_zui_001" />
            </item>
            <item android:duration="26">
                <bitmap android:src="@drawable/btn_radio_to_on_mtrl_zui_002" />
            </item>
            <item android:duration="26">
                <bitmap android:src="@drawable/btn_radio_to_on_mtrl_zui_003" />
            </item>
            <item android:duration="26">
                <bitmap android:src="@drawable/btn_radio_to_on_mtrl_zui_004" />
            </item>
            <item android:duration="26">
                <bitmap android:src="@drawable/btn_radio_to_on_mtrl_zui_005" />
            </item>
            <item android:duration="26">
                <bitmap android:src="@drawable/btn_radio_to_on_mtrl_zui_006" />
            </item>
            <item android:duration="26">
                <bitmap android:src="@drawable/btn_radio_to_on_mtrl_zui_007" />
            </item>
            <item android:duration="26">
                <bitmap android:src="@drawable/btn_radio_to_on_mtrl_zui_008" />
            </item>
            <item android:duration="26">
                <bitmap android:src="@drawable/btn_radio_to_on_mtrl_zui_009" />
            </item>
        </animation-list>
    </transition>
</animated-selector>